{
    "name": "@purinton/rabbitmq",
    "type": "module",
    "version": "1.0.1",
    "description": "A simple, testable RabbitMQ client for Node.js supporting both ESM and CommonJS.",
    "main": "index.cjs",
    "module": "index.mjs",
    "types": "index.d.ts",
    "files": [
        "index.mjs",
        "index.cjs",
        "index.d.ts",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.cjs",
            "types": "./index.d.ts"
        }
    },
    "scripts": {
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/rabbitmq.git"
    },
    "keywords": [
        "rabbitmq",
        "nodejs",
        "amqp",
        "messaging",
        "queue",
        "pubsub",
        "esmodule",
        "commonjs",
        "testable"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/rabbitmq/issues"
    },
    "homepage": "https://github.com/purinton/rabbitmq#readme",
    "devDependencies": {
        "jest": "^29.7.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/log": "^1.0.6",
        "amqplib": "^0.10.8"
    }
}